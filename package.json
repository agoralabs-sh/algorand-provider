{
  "name": "@agoralabs-sh/algorand-provider",
  "version": "1.1.0",
  "description": "Algorand provider is an interface that bridges the gap between dApps and wallets, allowing dApps to connect, and interact, with wallets in a standardized way.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "CONTRIBUTING.md",
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/agoralabs-sh/algorand-provider"
  },
  "author": {
    "name": "Kieran O'Neill",
    "email": "kieran@agoralabs.sh",
    "url": "https://github.com/kieranroneill"
  },
  "license": "MIT",
  "keywords": [
    "algorand",
    "algosdk",
    "blockchain"
  ],
  "private": false,
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "docs:build": "docusaurus build --out-dir .docs",
    "docs:serve": "docusaurus serve --dir .docs --no-open",
    "docs:start": "docusaurus start --no-open --port 8080",
    "lint": "eslint . --ext .ts --ext .js",
    "prepare": "husky install",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,json,ts}\"",
    "test": "jest"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@docusaurus/core": "2.3.1",
    "@docusaurus/module-type-aliases": "2.3.1",
    "@docusaurus/preset-classic": "2.3.1",
    "@docusaurus/remark-plugin-npm2yarn": "^2.3.1",
    "@faker-js/faker": "^7.6.0",
    "@mdx-js/react": "^1.6.22",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "clsx": "^1.2.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-release": "^20.1.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "algosdk": "^2.1.0"
  }
}
